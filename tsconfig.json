{
  "compilerOptions": {
    "module": "commonjs",
    "lib": [ "dom", "es2015" ],
    "moduleResolution": "node",
    "target": "es6",
    "experimentalDecorators": true,
    "emitDecoratorMetadata": true,
    "skipLibCheck": true
  },
  "exclude": [
    "node_modules",
    "library",
    "local",
    "temp",
    "build",
    "settings"
  ]
}